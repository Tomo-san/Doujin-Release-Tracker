{
	"name": "tomo-san/doujin-release-tracker",
	"description": "Webapp to keep tabs on Doujin Music releases at JP Events",
	"license": "THE STRONGEST PUBLIC LICENSE",
	"type": "project",
	"require": {
		"laravel/framework": "4.2.*",
		"dingo/api": "0.8.*"
	},
	"require-dev": {
		"way/generators": "~2.0"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		],
		"psr-0": {
			"Comiket": "app/"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
